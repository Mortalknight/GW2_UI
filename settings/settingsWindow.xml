<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

    <Frame name="GwSettingsWindowTmpl" frameStrata="HIGH" enableMouse="true" parent="UIParent" hidden="true" virtual="true" Mixin="GwSettingsWindowMixin">
        <HitRectInsets>
            <AbsInset left="-40" right="0" top="-40" bottom="0"/>
        </HitRectInsets>
        <Size x="853" y="627"></Size>
        <Anchors>
            <Anchor point="CENTER" x="0" y="0"></Anchor>
        </Anchors>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="background" file="Interface/AddOns/GW2_UI/textures/uistuff/settingspanelbg">
                    <Size x="853" y="853"></Size>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="0"></Anchor>
                    </Anchors>
                </Texture>
                <Texture file="Interface/AddOns/GW2_UI/textures/bag/bagheader">
                    <Size x="512" y="64"/>
                    <Anchors>
                        <Anchor point="BOTTOMLEFT" relativePoint="TOPLEFT" x="-40" y="0" />
                    </Anchors>
                </Texture>
                <Texture file="Interface/AddOns/GW2_UI/textures/bag/bagheader-right">
                    <Size x="512" y="64"/>
                    <Anchors>
                        <Anchor point="BOTTOMRIGHT" relativePoint="TOPRIGHT" x="0" y="0" />
                    </Anchors>
                </Texture>
                <Texture file="Interface/AddOns/GW2_UI/textures/bag/bagleftpanel">
                    <Size x="40" y="512"/>
                    <Anchors>
                        <Anchor point="TOPRIGHT" relativePoint="TOPLEFT" x="0" y="0" />
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMLEFT" x="0" y="0" />
                    </Anchors>
                </Texture>
            </Layer>
            <Layer level="BACKGROUND" textureSubLevel="1">
                <Texture file="Interface/AddOns/GW2_UI/textures/bag/bottom-right">
                    <Size x="128" y="128"/>
                    <Anchors>
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0" />
                    </Anchors>
                </Texture>
                <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/settingslogo">
                    <Size x="236" y="102"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="0" y="0" />
                    </Anchors>
                    <TexCoords left="0" right="0.921875 " top="0" bottom="0.796875"/>
                </Texture>
            </Layer>
            <Layer level="BACKGROUND" textureSubLevel="2">
                <Texture file="Interface/AddOns/GW2_UI/textures/character/settings-window-icon">
                    <Size x="84" y="84"></Size>
                    <Anchors>
                        <Anchor point="CENTER" relativePoint="TOPLEFT" x="-26" y="26" />
                    </Anchors>
                </Texture>
                <FontString parentKey="headerString" text="Settings" inherits="GameFontNormalSmall" justifyH="LEFT">
                    <Size x="0" y="40"/>
                    <Color r="1" b="0.73" g="0.93"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" x="30" y="40"/>
                    </Anchors>
                </FontString>
                <FontString parentKey="headerBreadcrumb" text="Settings" inherits="GameFontNormalSmall" justifyH="LEFT">
                    <Size x="0" y="40"/>
                    <Color r="1" b="0.73" g="0.93"/>
                    <Anchors>
                        <Anchor point="LEFT" relativeKey="$parent.headerString" relativePoint="RIGHT" x="10" y="-2"/>
                    </Anchors>
                </FontString>
                <FontString parentKey="versionString" text="Version" inherits="GameFontNormalSmall" justifyH="CENTER">
                    <Size x="236" y="40"/>
                    <Color r="1" b="0.73" g="0.93"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="0" y="-64"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Frames>
            <Frame parentKey="tabContent">
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="0" y="0" />
                    <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0" />
                </Anchors>
                <Frames>
                    <Frame parentKey="panel">
                        <Anchors>
                            <Anchor point="TOPLEFT" x="241" y="0"/>
                            <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0"></Anchor>
                        </Anchors>
                        <Layers>
                            <Layer level="BACKGROUND">
                                <Texture file="Interface\AddOns\GW2_UI\textures\uistuff\settingspanelsep">
                                    <Size x="4" y="512"/>
                                    <Anchors>
                                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="0" y="0"></Anchor>
                                    </Anchors>
                                </Texture>
                            </Layer>
                        </Layers>
                    </Frame>
                    <Frame parentKey="menu">
                        <Anchors>
                            <Anchor point="TOPLEFT" x="0" y="-102"></Anchor>
                            <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMLEFT" x="231" y="0"></Anchor>
                        </Anchors>
                        <Layers>
                            <Layer level="BACKGROUND" textureSubLevel="3">
                                <Texture file="Interface\AddOns\GW2_UI\textures\uistuff\236menubg">
                                    <Anchors>
                                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="0" y="0"></Anchor>
                                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0"></Anchor>
                                    </Anchors>
                                </Texture>
                            </Layer>
                        </Layers>
                    </Frame>
                </Frames>
            </Frame>
            <Frame parentKey="tabBar">
                <Size x="40" y="512"/>
                    <Anchors>
                        <Anchor point="TOPRIGHT" relativePoint="TOPLEFT" x="0" y="0" />
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMLEFT" x="0" y="0" />
                    </Anchors>
            </Frame>
            <Button parentKey="close" text="">
                <Size x="20" y="20" />
                <Anchors>
                    <Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" x="-10" y="30"/>
                </Anchors>
                <NormalTexture file="Interface/AddOns/GW2_UI/textures/uistuff/window-close-button-normal"/>
                <PushedTexture file="Interface/AddOns/GW2_UI/textures/uistuff/window-close-button-hover"/>
                <HighlightTexture file="Interface/AddOns/GW2_UI/textures/uistuff/window-close-button-hover"/>
                <NormalFont style="ChatBubbleFont"/>
                <HighlightFont style="ChatBubbleFont"/>
            </Button>
        </Frames>
    </Frame>

    <Button name="GwSettingsTabButtonTemplate" virtual="true">
        <Size x="64" y="40"></Size>
        <Layers>
            <Layer>
                <Texture parentKey="icon" file="Interface/AddOns/GW2_UI/textures/uistuff/tabicon_settings">
                    <Size x="64" y="40"/>
                    <Anchors>
                        <Anchor point="CENTER" x="0" y="0"/>
                    </Anchors>
                    <TexCoords left="0" right="0.5" top="0" bottom="0.625"/>
                </Texture>
            </Layer>
        </Layers>
    </Button>

</Ui>
