<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

    <Frame name="GwSettingsPartyPanelTmpl" inherits="GwSettingsPanelTmpl" virtual="true">
        <Frames>
            <Button parentKey="buttonPartyPreview" inherits="GwStandardButton" text="-">
                <Size x="24" y="24" />
                <Anchors>
                    <Anchor point="TOPRIGHT" x="-10" y="-10" />
                </Anchors>
                <Layers>
                    <Layer level="OVERLAY">
                        <FontString parentKey="label" text="PREVIEW" inherits="GameFontNormalSmall" justifyH="RIGHT">
                            <Color r="1" b="1" g="1"></Color>
                            <Anchors>
                                <Anchor point="RIGHT" relativePoint="LEFT" x="-5" y="0"></Anchor>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
            </Button>
        </Frames>
    </Frame>

</Ui>
