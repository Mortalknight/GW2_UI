                                <Size x="12" y="12"/>
                                    <Anchors>
                                        <Anchor point="TOP" relativePoint="TOP"/>
                                    </Anchors>
                                </Texture>
                            </Layer>
                        </Layers>
                    </Button>
                    <Button parentKey="scrollDown">
                        <Size x="12" y="12"/>
                        <Anchors>
                            <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0"/>
                        </Anchors>
                        <Layers>
