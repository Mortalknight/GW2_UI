                                        <Anchor point="TOP" relativePoint="TOP"/>
                                        <Anchor point="BOTTOM" relativePoint="BOTTOM"/>
                                    </Anchors>
                                </Texture>
                            </Layer>
                        </Layers>
                        <Scripts>
                            <OnValueChanged>
                                self:GetParent():SetVerticalScroll(value)
