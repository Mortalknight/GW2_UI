                                    </Anchors>
                                    <TexCoords left="0" right="1" top="1" bottom="0" />
                                </Texture>
                            </Layer>
                        </Layers>
                    </Button>
                </Frames>
                <Scripts>
                    <OnMouseWheel>
                        delta = -delta * 15
                        local s = math.max(0, self:GetVerticalScroll() + delta)
                        s = math.min(self.maxScroll, s)
                        self:SetVerticalScroll(s)
                        self.slider:SetValue(s)
                    </OnMouseWheel>
                </Scripts>
            </ScrollFrame>
        </Frames>
