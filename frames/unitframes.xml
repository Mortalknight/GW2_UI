    <Button name="GwAuraFrame" enabledMouse="true" registerForClicks="RightButtonDown" hidden="true" virtual="true">
        <Size x="32" y="32"></Size>
        <Anchors>
            <Anchor point="CENTER" relativePoint="TOPLEFT"></Anchor>
        </Anchors>
        <Layers>
            <Layer level="ARTWORK" textureSubLevel="-1">
                <Texture parentKey="backdrop" file="Interface\AddOns\GW2_UI\textures\gwstatusbar">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="-1" y="1"></Anchor>
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="1" y="-1"></Anchor>
                    </Anchors>
                <Color r="0" g="0" b="0"></Color>
                </Texture>  
            </Layer>
            <Layer level="ARTWORK">
                <Texture parentKey="background" file="Interface\AddOns\GW2_UI\textures\gwstatusbar">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT"></Anchor>
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT"></Anchor>
                    </Anchors>
                <Color r="0" g="0" b="0"></Color>
                </Texture>                
            </Layer>
        </Layers>
        <Frames>
            <Cooldown  parentKey="cooldown" inherits="ActionButtonTemplate">
                <SwipeTexture>
                    <Color r="1" g="1" b="1" a="1"/>
                </SwipeTexture>
                
                <Anchors>
                    <Anchor point="TOPLEFT"  relativePoint="TOPLEFT" x="1" y="-1"/>
                    <Anchor point="BOTTOMRIGHT"  relativePoint="BOTTOMRIGHT" x="-1" y="1"/>
                </Anchors>
            </Cooldown>
            <Frame>
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="1" y="-1"></Anchor>
                    <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="-1" y="1"></Anchor>
                </Anchors>
                <Layers> 
                    <Layer level="OVERLAY">
                        <Texture parentKey="icon" file="Interface\AddOns\GW2_UI\textures\gwstatusbar">
                            <Anchors>
                                <Anchor point="TOPLEFT" relativePoint="TOPLEFT"></Anchor>
                                <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT"></Anchor>
                            </Anchors>
                             <TexCoords left="0.1" right="0.9" top="0.1" bottom="0.9" />
                        </Texture>
                    </Layer>
                    <Layer level="OVERLAY" textureSubLevel="1">
                        <Texture  file="Interface\AddOns\GW2_UI\textures\icon-overlay" >
                            <Anchors>
                                <Anchor point="TOPLEFT" relativeKey="$parent.icon" relativePoint="TOPLEFT" x="0" y="0"/>
                                <Anchor point="BOTTOMRIGHT" relativeKey="$parent.icon" relativePoint="BOTTOMRIGHT" x="0" y="0"/>
                            </Anchors>
                        </Texture>
                    </Layer>
                    <Layer level="OVERLAY" textureSubLevel="2">
                        <FontString parentKey="stacks" text="20" justifyH="CENTER" justifyV="BOTTOM">
                            <Anchors>
                                <Anchor point="TOPLEFT" relativeKey="$parent.icon" relativePoint="TOPLEFT" x="-10" y="0"/>
                                <Anchor point="BOTTOMRIGHT" relativeKey="$parent.icon" relativePoint="BOTTOMRIGHT" x="10" y="0"/>
                            </Anchors>
                        </FontString> 
                        <FontString parentKey="duration" text="0.00" justifyH="CENTER" justifyV="BOTTOM">
                            <Size x="1" y="14"></Size>
                            <Anchors>
                                <Anchor point="TOPLEFT"  relativePoint="BOTTOMLEFT" x="-10" y="0"/>
                                <Anchor point="TOPRIGHT"  relativePoint="BOTTOMRIGHT" x="10" y="0"/>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
                <Scripts>
                    <OnLoad>
                        self.stacks:SetFont(UNIT_NAME_FONT,11,'OUTLINED')
                        self.duration:SetFont(UNIT_NAME_FONT,10)
                        self.duration:SetShadowOffset(1,-1)
            
                        self:GetParent().duration = self.duration
                        self:GetParent().stacks = self.stacks
                        self:GetParent().icon = self.icon
                    </OnLoad>
                </Scripts>
            </Frame>
        </Frames>
        
        <Scripts>
            <OnLoad>
                self.typeAura = 'smallbuff'
                self.cooldown:SetDrawEdge(0)
                self.cooldown:SetDrawSwipe(1)
                self.cooldown:SetReverse(false)
                self.cooldown:SetHideCountdownNumbers(true)
                self.throt = 0
            </OnLoad>
            <OnUpdate>
                if GetTime() > self.throt and self:IsShown() and self.expires~=nil then
                    self.throt = GetTime() + 0.2
                    self.duration:SetText( timeCount( self.expires - GetTime()) )
                    
                  
                end
            </OnUpdate>
            <OnEnter>
                if self:IsShown() and self.auraid~=nil and self.unit~=nil then
                
                    GameTooltip:SetOwner(self, "ANCHOR_BOTTOMLEFT")
                    GameTooltip:ClearLines()
                    if self.auraType == 'buff' then
                        GameTooltip:SetUnitBuff(self.unit,self.auraid)
                    else
                        GameTooltip:SetUnitDebuff(self.unit,self.auraid,self.debuffFilter)
                    end
                    GameTooltip:Show()
                end
            </OnEnter>
            <OnLeave>
                GameTooltip:Hide()
            </OnLeave>
            <OnClick>
                if not InCombatLockdown() and self.auraType == 'buff' and button=='RightButton' and self.unit=='player' then
                    CancelUnitBuff("player",self.auraid)
                end
            </OnClick>
        </Scripts>
        
    </Button>
    
    
    
