    <Frame name="GwMiniMapTrackingFrame" enableMouse="true" hidden="true" virtual="true">
        <Size>
            <AbsDimension x="33" y="33"/>
        </Size>
        <Anchors>
            <Anchor point="TOPLEFT">
                <Offset>
                    <AbsDimension x="-15" y="0"/>
                </Offset>
            </Anchor>
        </Anchors>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="icon" file="Interface\AddOns\GW2_UI\Textures\track-icons-map">
                    <Size>
                        <AbsDimension x="40" y="40"/>
                    </Size>
                    <Anchors>
                        <Anchor point="TOPLEFT"/>
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
        <Scripts>
            <OnMouseUp>
                ToggleDropDownMenu(1, nil, MiniMapTrackingDropDown, "MiniMapTracking", 0, -5)
            </OnMouseUp> 
            <OnEnter>
                GameTooltip:SetOwner(self, "ANCHOR_LEFT");
                GameTooltip:SetText(TRACKING, 1, 1, 1);
                GameTooltip:AddLine(MINIMAP_TRACKING_TOOLTIP_NONE, nil, nil, nil, true);
                GameTooltip:Show();
            </OnEnter>
            <OnLeave>
                GameTooltip:Hide()
            </OnLeave>
        </Scripts>
    </Frame>

