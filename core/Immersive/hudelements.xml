    <Button name="GwAlertFrameOffsetter" toplevel="true" parent="UIParent" frameStrata="BACKGROUND" frameLevel="1" enableMouse="false" hidden="true">
        <Size x="800" y="140"/>
        <Anchors>
            <Anchor point="BOTTOM" x="0" y="300"/>
        </Anchors>
    </Button>

    <Button name="GwAddonToggle" frameStrata="MEDIUM" virtual="true">
        <Size x="38" y="38"/>
        <NormalTexture file="Interface/AddOns/GW2_UI/textures/icons/addondropout.png">
            <Size x="38" y="38"></Size>
            <Anchors>
                <Anchor point="CENTER"></Anchor>
            </Anchors>
        </NormalTexture>
        <HighlightTexture file="Interface/AddOns/GW2_UI/textures/icons/addondropout.png" alphaMode="ADD" alpha="0.5">
            <Size x="38" y="38"></Size>
            <Anchors>
