                </Texture>
            </Layer>
        </Layers>
        <Frames>
            <StatusBar parentKey="progress" inherits="GwStatusBarBar">
                 <Size x="176" y="15"></Size>
                 <BarTexture parentKey="internalBar" file="Interface/AddOns/GW2_UI/textures/units/castingbars/yellow-norm.png"/>
                <BarColor r="1" g="1" b ="1" a="1"/>
