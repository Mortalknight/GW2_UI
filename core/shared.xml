                <Anchors>
                    <Anchor point="TOPRIGHT" relativePoint="TOPLEFT" x="0" y="0" />
                    <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMLEFT" x="0" y="0" />
                </Anchors>
                </Texture>
            </Layer>
        </Layers>
    </Frame>


</Ui>
