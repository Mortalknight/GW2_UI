<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

    <Font name="gw_button_font_black" inherits="SystemFont_Med1" virtual="true"> <!-- size 12 -->
        <Color r="0" b="0" g="0"></Color>
    </Font>

    <Font name="gw_button_font_black_small" inherits="SystemFont_Small" virtual="true"> <!-- size 10 -->
        <Color r="0" b="0" g="0"></Color>
    </Font>

    <Font name="gw_button_font_menu_light" inherits="SystemFont_Large" virtual="true"> <!-- size 16 -->
        <Color r="1" g="0.9450" b="0.8196" ></Color>
    </Font>

    <Frame name="GwLightButtonBorder" virtual="true">
        <Size x="170" y="64"/>
        <Anchors>
        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="0" y="0" />
        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0" />
        </Anchors>
        <Layers>
            <Layer level="ARTWORK" textureSubLevel="1">
            <Texture parentKey="left" file="Interface\AddOns\GW2_UI\textures\uistuff\buttonlight-bordervertical.png">
                <Size x="1" y="84"></Size>
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="0" y="0" />
                    <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" x="0" y="0" />
                </Anchors>
            </Texture>
            <Texture parentKey="right" file="Interface\AddOns\GW2_UI\textures\uistuff\buttonlight-bordervertical.png">
                <Size x="1" y="84"></Size>
                <Anchors>
                    <Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" x="0" y="0" />
                    <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0" />
                </Anchors>
                <TexCoords left="1" right="0" top="0" bottom="1" />
            </Texture>
            <Texture parentKey="top" file="Interface\AddOns\GW2_UI\textures\uistuff\buttonlight-borderhorizontal.png">
                <Size x="2" y="1"></Size>
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="0" y="0" />
                    <Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" x="0" y="0" />
                </Anchors>
            </Texture>
            <Texture parentKey="bottom" hidden="true" file="Interface\AddOns\GW2_UI\textures\uistuff\buttonlight-borderhorizontal.png">
                <Size x="2" y="1"></Size>
                <Anchors>
                    <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" x="0" y="0" />
                    <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0" />
                </Anchors>
            </Texture>
            </Layer>
        </Layers>
    </Frame>

    <Frame name="GwButtonBorder" virtual="true">
        <Layers>
        <Layer level="BACKGROUND">
                <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/statusbarborderpixel.png">
                    <Size x="2" y="2"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="-1" y="2"/>
                        <Anchor point="TOPRIGHT" relativePoint="TOPRIGHT"  x="1" y="2"/>
                    </Anchors>
                </Texture>
                <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/statusbarborderpixel.png">
                    <Size x="2" y="2"/>
                    <Anchors>
                        <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" x="-1" y="-2"/>
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT"  x="1" y="-2"/>
                    </Anchors>
                    <TexCoords left="0" right="1" top="1" bottom="0" />
                </Texture>
                <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/statusbarborderpixelvertical.png">
                    <Size x="2" y="2"/>
                    <Anchors>
                        <Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" x="2" y="0"/>
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="2" y="0"/>
                    </Anchors>
                </Texture>
                <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/statusbarborderpixelvertical.png">
                    <Size x="2" y="2"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="-2" y="0"/>
                        <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" x="-2" y="0"/>
                    </Anchors>
                    <TexCoords left="1" right="0" top="0" bottom="1" />
                </Texture>
            </Layer>
        </Layers>
    </Frame>

    <Button name="GwStandardButton" text="" virtual="true">
        <Size x="128" y="24" />
        <NormalTexture file="Interface/AddOns/GW2_UI/textures/uistuff/button.png"/>
        <DisabledTexture file="Interface/AddOns/GW2_UI/textures/uistuff/button_disable.png"/>
        <NormalFont style="gw_button_font_black"/>
        <HighlightFont style="gw_button_font_black"/>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/statusbarborderpixel.png">
                    <Size x="318" y="2"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="-1" y="2"/>
                        <Anchor point="TOPRIGHT" relativePoint="TOPRIGHT"  x="1" y="2"/>
                    </Anchors>
                </Texture>
                <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/statusbarborderpixel.png">
                    <Size x="318" y="2"/>
                    <Anchors>
                        <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" x="-1" y="-2"/>
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT"  x="1" y="-2"/>
                    </Anchors>
                    <TexCoords left="0" right="1" top="1" bottom="0" />
                </Texture>
                <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/statusbarborderpixelvertical.png">
                    <Size x="2" y="2"/>
                    <Anchors>
                        <Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" x="2" y="0"/>
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="2" y="0"/>
                    </Anchors>
                </Texture>
                <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/statusbarborderpixelvertical.png">
                    <Size x="2" y="2"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="-2" y="0"/>
                        <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" x="-2" y="0"/>
                    </Anchors>
                    <TexCoords left="1" right="0" top="0" bottom="1" />
                </Texture>
            </Layer>
            <Layer level="ARTWORK" textureSubLevel="7">
                <Texture parentKey="hover" file="Interface/AddOns/GW2_UI/textures/uistuff/button_hover.png">
                    <Anchors>
                        <Anchor point="LEFT" relativePoint="LEFT"/>
                        <Anchor point="TOP" relativePoint="TOP"/>
                        <Anchor point="BOTTOM" relativePoint="BOTTOM"/>
                        <Anchor point="RIGHT" relativePoint="LEFT"/>
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
        <Scripts>
            <OnShow>
                self:HookScript("OnEnter", GwStandardButton_OnEnter)
                self:HookScript("OnLeave", GwStandardButton_OnLeave)
            </OnShow>
        </Scripts>
    </Button>
    <Button name="GwStandardButtonNoOutline" text="" virtual="true">
        <Size x="128" y="24" />
        <NormalTexture file="Interface/AddOns/GW2_UI/textures/uistuff/button.png"/>
        <DisabledTexture file="Interface/AddOns/GW2_UI/textures/uistuff/button_disable.png"/>
        <NormalFont style="gw_button_font_menu_light"/>
        <HighlightFont style="gw_button_font_menu_light"/>
        <Layers>
            <Layer level="ARTWORK" textureSubLevel="7">
                <Texture parentKey="hover" file="Interface/AddOns/GW2_UI/textures/uistuff/button_hover.png">
                    <Anchors>
                        <Anchor point="LEFT" relativePoint="LEFT"/>
                        <Anchor point="TOP" relativePoint="TOP"/>
                        <Anchor point="BOTTOM" relativePoint="BOTTOM"/>
                        <Anchor point="RIGHT" relativePoint="LEFT"/>
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
        <Scripts>
            <OnShow>
                self:HookScript("OnEnter", GwStandardButton_OnEnter)
                self:HookScript("OnLeave", GwStandardButton_OnLeave)
            </OnShow>
        </Scripts>
    </Button>

    <PlayerModel name="GW2ModelLevelTemplate" useParentLevel="true" virtual="true"/>

    <Slider name="GwHybridScrollBarTemplate" parentKey="scrollBar" virtual="true">
        <Size x="12" y="0"></Size>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="trackBG" file="Interface/AddOns/GW2_UI/textures/uistuff/scrollbg.png">
                    <Anchors>
                        <Anchor point="TOPRIGHT" x="-3"></Anchor>
                        <Anchor point="BOTTOMRIGHT" x="-3"></Anchor>
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
        <Frames>
            <Button parentKey="scrollUp">
                <Size x="12" y="12"></Size>
                <Anchors>
                    <Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" x="0" y="12"></Anchor>
                </Anchors>
                <Layers>
                    <Layer>
                        <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/scrollbutton.png">
                            <Size x="12" y="12"></Size>
                            <Anchors>
                                <Anchor point="TOP" relativePoint="TOP"></Anchor>
                            </Anchors>
                        </Texture>
                    </Layer>
                </Layers>
                <Scripts>
                    <OnLoad function="HybridScrollFrameScrollUp_OnLoad"/>
                    <OnClick function="HybridScrollFrameScrollButton_OnClick"/>
                </Scripts>
            </Button>

            <Button parentKey="scrollDown">
                <Size x="12" y="12"></Size>
                <Anchors>
                    <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="-12"></Anchor>
                </Anchors>
                <Layers>
                    <Layer>
                        <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/scrollbutton.png">
                            <Size x="12" y="12"></Size>
                            <Anchors>
                                <Anchor point="TOP" relativePoint="TOP"></Anchor>
                            </Anchors>
                            <TexCoords left="0" right="1" top="1" bottom="0" />
                        </Texture>
                    </Layer>
                </Layers>
                <Scripts>
                    <OnLoad function="HybridScrollFrameScrollDown_OnLoad"/>
                    <OnClick function="HybridScrollFrameScrollButton_OnClick"/>
                </Scripts>
            </Button>
        </Frames>
        <Scripts>
            <OnValueChanged function="HybridScrollFrame_OnValueChanged"/>
        </Scripts>
        <ThumbTexture parentKey="thumbTexture" file="Interface/AddOns/GW2_UI/textures/uistuff/scrollbarmiddle.png">
            <Size x="12" y="100"/>
            <Anchors>
                <Anchor point="TOP" relativePoint="TOP"></Anchor>
            </Anchors>
        </ThumbTexture>
    </Slider>

    <ScrollFrame name="GwHybridScrollFrameTemplate" inherits="HybridScrollFrameTemplate" virtual="true">
        <Frames>
            <Slider inherits="GwHybridScrollBarTemplate">
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="TOPRIGHT" x="0" y="-12"></Anchor>
                    <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMRIGHT" x="0" y="12"></Anchor>
                </Anchors>
            </Slider>
        </Frames>
    </ScrollFrame>

    <Frame name="GwFrameHeader" virtual="true">
        <Size x="170" y="64"/>
        <Anchors>
            <Anchor point="BOTTOMLEFT" relativePoint="TOPLEFT" x="0" y="-32" />
            <Anchor point="BOTTOMRIGHT" relativePoint="TOPRIGHT" x="0" y="-32" />
        </Anchors>
        <Layers>
        <Layer level="ARTWORK" textureSubLevel="1">
            <Texture parentKey="windowIcon" file="Interface\AddOns\GW2_UI\textures\character\worldmap-window-icon.png">
                <Size x="84" y="84"></Size>
                <Anchors>
                    <Anchor point="CENTER" relativeKey="$parent.BGLEFT" relativePoint="LEFT" x="21" y="0" />
                </Anchors>
            </Texture>
            </Layer>
            <Layer level="BACKGROUND" textureSubLevel="2">
                <Texture parentKey="BGLEFT" file="Interface/AddOns/GW2_UI/textures/bag/bagheader.png">
                <Size x="512" y="64"></Size>
                    <Anchors>
                        <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" x="0" y="0" />
                    </Anchors>
                </Texture>
                <Texture parentKey="BGRIGHT" file="Interface/AddOns/GW2_UI/textures/bag/bagheader-right.png">
                    <Size x="512" y="64"></Size>
                    <Anchors>
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0" />
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
    </Frame>

    <EditBox name="GwInputBoxInstructionsTemplate" virtual="true">
        <Layers>
            <Layer level="ARTWORK">
                <FontString parentKey="Instructions" justifyH="LEFT" justifyV="MIDDLE" inherits="GameFontDisableSmall" setAllPoints="true">
                    <Color r="0.35" g="0.35" b="0.35"/>
                </FontString>
            </Layer>
        </Layers>
        <Scripts>
            <OnTextChanged function="InputBoxInstructions_OnTextChanged"/>
            <OnDisable function="InputBoxInstructions_OnDisable"/>
            <OnEnable function="InputBoxInstructions_OnEnable"/>
        </Scripts>
        <FontString inherits="GameFontHighlightSmall"/>
    </EditBox>

    <EditBox name="GwSearchBoxTemplate" inherits="GwInputBoxInstructionsTemplate" autoFocus="false" virtual="true">
        <Size x="214" y="25"></Size>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/gwstatusbar-bg.png">
                    <Anchors>
                        <Anchor point="TOPLEFT" x="-3" y="0"></Anchor>
                        <Anchor point="BOTTOMRIGHT" x="3" y="0"></Anchor>
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
        <FontString inherits="ChatFontNormal"/>
        <Frames>
            <Button parentKey="clearButton" hidden="true">
                <Size x="17" y="17"/>
                <Anchors>
                    <Anchor point="RIGHT" x="-3" y="0"/>
                </Anchors>
                <Layers>
                    <Layer level="ARTWORK">
                        <Texture atlas="common-search-clearbutton" useAtlasSize="false" alpha="0.5" parentKey="texture">
                            <Size x="10" y="10"/>
                            <Anchors>
                                <Anchor point="TOPLEFT" x="3" y="-3"/>
                            </Anchors>
                        </Texture>
                    </Layer>
                </Layers>
                <Scripts>
                    <OnEnter>
                        self.texture:SetAlpha(1.0)
                    </OnEnter>
                    <OnLeave>
                        self.texture:SetAlpha(0.5)
                    </OnLeave>
                    <OnMouseDown>
                        if self:IsEnabled() then
                            self.texture:SetPoint("TOPLEFT", self, "TOPLEFT", 4, -4)
                        end
                    </OnMouseDown>
                    <OnMouseUp>
                        self.texture:SetPoint("TOPLEFT", self, "TOPLEFT", 3, -3)
                    </OnMouseUp>
                </Scripts>
            </Button>
        </Frames>
    </EditBox>

     <Button name="GwHeroPanelMenuButtonTemplate" inherits="GwStandardButtonNoOutline" virtual="true">
        <Size x="231" y="36"></Size>
        <Anchors>
            <Anchor point="TOPLEFT" relativePoint="TOPLEFT"></Anchor>
        </Anchors>
        <NormalTexture file="Interface\AddOns\GW2_UI\textures\character\menu-bg.png"></NormalTexture>
        <Layers>
            <Layer textureSubLevel="OVERLAY">
                <Texture parentKey="activeTexture" file="Interface\AddOns\GW2_UI\textures\character\menu-hover.png" hidden="true">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT"></Anchor>
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT"></Anchor>
                    </Anchors>
                </Texture>
            </Layer>
            <Layer>
                <Texture parentKey="arrow" file="Interface\AddOns\GW2_UI\textures\character\menu-arrow.png">
                    <Size x="10" y="20"></Size>
                    <Anchors>
                        <Anchor point="RIGHT" relativePoint="RIGHT"></Anchor>
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
    </Button>

    <Button name="GwHeroPanelMenuButtonBackTemplate" inherits="GwStandardButtonNoOutline" virtual="true">
        <Layers>
            <Layer level="OVERLAY" textureSubLevel="8">
                <Texture parentKey="backarrow" file="Interface\AddOns\GW2_UI\textures\character\backicon.png">
                    <Size x="32" y="32"></Size>
                    <Anchors>
                        <Anchor point="LEFT" relativePoint="LEFT"></Anchor>
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
    </Button>

    <Frame name="GwHeroPanelMenuTemplate" virtual="true">
        <Size x="231" y="498"></Size>
        <Anchors>
            <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="10" y="-20"></Anchor>
        </Anchors>
    </Frame>

    <Frame name="GwStandardInputField" virtual="true">
        <Size x="226" y="26"/>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="background" file="Interface/AddOns/GW2_UI/textures/uistuff/statusbar.png">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="0" y="0"></Anchor>
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0"></Anchor>
                    </Anchors>
                    <Color r="1" g="1" b="1" a="0.5"></Color>
                </Texture>
                <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/statusbarborderpixel.png">
                    <Size x="314" y="1"/>
                    <Anchors>
                        <Anchor point="TOPLEFT"   relativeKey="$parent.background" relativePoint="TOPLEFT" x="0" y="0"/>
                        <Anchor point="TOPRIGHT" relativeKey="$parent.background" relativePoint="TOPRIGHT"  x="0" y="0"/>
                    </Anchors>
                </Texture>
                <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/statusbarborderpixel.png">
                    <Size x="314" y="1"/>
                    <Anchors>
                        <Anchor point="BOTTOMLEFT" relativeKey="$parent.background" relativePoint="BOTTOMLEFT" x="0" y="0"/>
                        <Anchor point="BOTTOMRIGHT" relativeKey="$parent.background" relativePoint="BOTTOMRIGHT"  x="0" y="0"/>
                    </Anchors>
                    <TexCoords left="0" right="1" top="1" bottom="0" />
                </Texture>

                <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/statusbarborderpixelvertical.png">
                    <Size x="1" y="2"/>
                    <Anchors>
                        <Anchor point="TOPRIGHT" relativeKey="$parent.background" relativePoint="TOPRIGHT" x="1" y="0"/>
                        <Anchor point="BOTTOMRIGHT" relativeKey="$parent.background" relativePoint="BOTTOMRIGHT" x="1" y="0"/>
                    </Anchors>
                </Texture>
                <Texture file="Interface/AddOns/GW2_UI/textures/uistuff/statusbarborderpixelvertical.png">
                    <Size x="2" y="2"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativeKey="$parent.background" relativePoint="TOPLEFT" x="-1" y="0"/>
                        <Anchor point="BOTTOMLEFT" relativeKey="$parent.background" relativePoint="BOTTOMLEFT" x="-1" y="0"/>
                    </Anchors>
                    <TexCoords left="1" right="0" top="0" bottom="1" />
                </Texture>
            </Layer>
        </Layers>
        <Frames>
            <EditBox parentKey="input" autoFocus="false">
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="5" y="0"></Anchor>
                    <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="-5" y="0"></Anchor>
                </Anchors>
                <FontString inherits="GameFontNormalSmall">
                    <Color r="1" b="1" g="1"/>
                </FontString>
                <Frames>
                    <Button parentKey="clearButton" hidden="true">
                        <Size x="17" y="17"/>
                        <Anchors>
                            <Anchor point="RIGHT" x="-3" y="0"/>
                        </Anchors>
                        <Layers>
                            <Layer level="ARTWORK">
                                <Texture atlas="common-search-clearbutton" useAtlasSize="false" alpha="0.5" parentKey="texture">
                                    <Size x="10" y="10"/>
                                    <Anchors>
                                        <Anchor point="TOPLEFT" x="3" y="-3"/>
                                    </Anchors>
                                </Texture>
                            </Layer>
                        </Layers>
                        <Scripts>
                            <OnEnter>
                                self.texture:SetAlpha(1.0);
                            </OnEnter>
                            <OnLeave>
                                self.texture:SetAlpha(0.5);
                            </OnLeave>
                            <OnMouseDown>
                                if self:IsEnabled() then
                                    self.texture:SetPoint("TOPLEFT", self, "TOPLEFT", 4, -4);
                                end
                            </OnMouseDown>
                            <OnMouseUp>
                                self.texture:SetPoint("TOPLEFT", self, "TOPLEFT", 3, -3);
                            </OnMouseUp>
                        </Scripts>
                    </Button>
                </Frames>
            </EditBox>
        </Frames>
    </Frame>

