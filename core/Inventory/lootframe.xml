<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

    <Frame name="GwLootFrameTitleTemp" virtual="true">
        <Size x="170" y="64"/>
        <Anchors>
            <Anchor point="BOTTOMLEFT" relativePoint="TOPLEFT"/>
        </Anchors>
        <Layers>
          <Layer level="ARTWORK" textureSubLevel="1">
              <Texture parentKey="windowIcon" file="Interface\AddOns\GW2_UI\textures\character\loot-window-icon">
                  <Size x="84" y="84"></Size>
                  <Anchors>
                      <Anchor point="CENTER" relativeKey="$parent.BGLEFT" relativePoint="LEFT" x="21" y="0" />
