                <Texture parentKey="BGRIGHT" file="Interface/AddOns/GW2_UI/textures/bag/bagheader-right">
                    <Size x="170" y="41"></Size>
                    <Anchors>
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="2" y="0" />
                    </Anchors>
                    <TexCoords left="0.7" right="1" top="0.35" bottom="1"/>
                </Texture>
            </Layer>
            <Layer level="BORDER">
                <FontString parentKey="headerString" text="ITEMS" inherits="GameFontNormalSmall" justifyH="LEFT">
                    <Size x="170" y="41"></Size>
                    <Color r="1" b="0.73" g="0.93"></Color>
                    <Anchors>
                        <Anchor point="LEFT" relativePoint="LEFT" relativeKey="$parent.BGLEFT" x="54" y="0" />
