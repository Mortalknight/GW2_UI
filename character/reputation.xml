                                    </Anchors>
                                </Texture>
                            </Layer>
                            <Layer level="OVERLAY" textureSubLevel="2">
                                <FontString parentKey="percentage" hidden="true" text="Unknown" justifyH="CENTER">
                                    <Size x="180" y="20"></Size>
                                    <Anchors>
                                        <Anchor point="CENTER" relativePoint="CENTER" x="0" y="0"></Anchor>
                                    </Anchors>
                                </FontString>
                            </Layer>
                        </Layers>
                    </StatusBar>
                </Frames>
            </Button>
        </Frames>
    </Button>

