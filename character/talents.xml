                    if MouseIsOver(self) then
                        local r, g, b, a = self.background:GetVertexColor()
                        local r2, g2, b2, a2 = self.backgroundFx:GetVertexColor()
                        r = math.min(0.8, r + (1 * elapsed))
                        r2 = math.min(1, r2 + (1 * elapsed))
                        self.background:SetVertexColor(r, r, r, r)
                        self.backgroundFx:SetVertexColor(r2, r2, r2, r2)
                        return
                    end
                    self.background:SetVertexColor(0.7, 0.7, 0.7, 0.5)
                    self.backgroundFx:SetVertexColor(1,1,1, 0.5)
                end)
            </OnShow>
            <OnHide>
                self:SetScript('OnUpdate', nil)
