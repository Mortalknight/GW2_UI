<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">

    <Button name="GwCurrencyRow" virtual="true">
        <Size x="512" y="32"></Size>
        <Anchors>
            <Anchors point="TOPLEFT"></Anchors>
        </Anchors>
        <Frames>
            <Frame parentKey="header" setAllPoints="true">
                <Layers>
                    <Layer>
                        <Texture name="$parentBackground" file="Interface\AddOns\GW2_UI\textures\bag-sep">
                            <Size x="512" y="32"></Size>
                            <Anchors>
                                <Anchors point="TOPLEFT"></Anchors>
                            </Anchors>
                        </Texture>
                    </Layer>
                    <Layer textureSubLevel="2">
                        <FontString parentKey="spaceString" text="Name Could not be loaded" inherits="GameFontNormalSmall" justifyV="CENTER" justifyH="LEFT">
                            <Anchors>
                                <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="10" y="-5" />
                            </Anchors>
                            <Size x="200" y="24" />
                        </FontString>
                    </Layer>
                </Layers>
            </Frame>
            <Button parentKey="item" setAllPoints="true" hidden="true">
                <Layers>
                    <Layer level="ARTWORK">
                        <Texture parentKey="zebra" file="Interface\AddOns\GW2_UI\textures\gwstatusbar">
                            <Size x="32" y="32"></Size>
                            <Anchors>
                                <Anchors point="TOPLEFT" relativePoint="TOPLEFT"></Anchors>
                                <Anchors point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT"></Anchors>
                            </Anchors>
                        </Texture>
                        <Texture parentKey="icon">
                            <Size x="32" y="32"></Size>
                            <Anchors>
                                <Anchors point="TOPLEFT" relativePoint="TOPLEFT"></Anchors>
                            </Anchors>
                            <TexCoords left="0.1" right="0.9" top="0.1" bottom="0.9" />
                        </Texture>
                    </Layer>
                    <Layer textureSubLevel="2">
                        <FontString parentKey="spaceString" text="Name Could not be loaded" inherits="GameFontNormalSmall" justifyV="CENTER" justifyH="LEFT">
                            <Anchors>
                                <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="42" y="-5" />
                            </Anchors>
                            <Size x="350" y="24" />
                        </FontString>
                        <FontString parentKey="amount" text="Amount Could not be loaded" inherits="GameFontNormalSmall" justifyV="CENTER" justifyH="RIGHT">
                            <Anchors>
                                <Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" x="-5" y="-5" />
                            </Anchors>
                            <Size x="200" y="24" />
                        </FontString>
                    </Layer>
                </Layers>
            </Button>
        </Frames>
    </Button>

    <Button name="GwRaidInfoRow" virtual="true">
        <Size x="512" y="55"></Size>
        <Anchors>
            <Anchors point="TOPLEFT"></Anchors>
        </Anchors>
        <Frames>
            <Button parentKey="item" setAllPoints="true" hidden="true">
                <Layers>
                    <Layer level="ARTWORK">
                        <Texture parentKey="zebra" file="Interface\AddOns\GW2_UI\textures\gwstatusbar">
                            <Size x="32" y="55"></Size>
                            <Anchors>
                                <Anchors point="TOPLEFT" relativePoint="TOPLEFT"></Anchors>
                                <Anchors point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT"></Anchors>
                            </Anchors>
                        </Texture>
                    </Layer>
                    <Layer textureSubLevel="2">
                        <FontString parentKey="name" text="Name Could not be loaded" inherits="GameFontNormalSmall" justifyV="TOP" justifyH="LEFT">
                            <Anchors>
                                <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="5" y="-5" />
                            </Anchors>
                            <Size x="300" y="22" />
                        </FontString>
                        <FontString parentKey="difficult" text="Heroic" inherits="GameFontNormalSmall" justifyV="BOTTOM" justifyH="Left">
                            <Anchors>
                                <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" x="15" y="20" />
                            </Anchors>
                            <Size x="200" y="22" />
                        </FontString>
                        <FontString parentKey="reset" text="????" inherits="GameFontNormalSmall" justifyV="TOP" justifyH="RIGHT">
                            <Anchors>
                                <Anchor point="TOPRIGHT" relativePoint="TOPRIGHT" x="-15" y="-5" />
                            </Anchors>
                            <Size x="200" y="22" />
                        </FontString>
                        <FontString parentKey="extended" text="" inherits="GameFontNormalSmall" justifyV="BOTTOM" justifyH="RIGHT">
                            <Anchors>
                                <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="-15" y="20" />
                            </Anchors>
                            <Size x="200" y="22" />
                        </FontString>
                    </Layer>
                </Layers>
                <Frames>
                    <Button parentKey="extendButton" inherits="GwStandardButton" text="???" hidden="true" style="ChatBubbleFont">
                        <Size x="210" y="13"/>
                        <Anchors>
                            <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" x="15" y="5"/>
                        </Anchors>
                    </Button>
                </Frames>
            </Button>
        </Frames>
    </Button>

    <Frame name="GwCurrencyWindow" virtual="true">
        <Size x="608" y="621"></Size>
        <Anchors>
            <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="245" y="-5"></Anchor>
        </Anchors>
        <Layers>
            <Layer level="ARTWORK">
                <Texture file="Interface\AddOns\GW2_UI\textures\character\paperdollbg">
                    <Size x="608" y="621"></Size>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="0" y="0"></Anchor>
                    </Anchors>
                </Texture>
            </Layer>
        </Layers>
        <Frames>
            <ScrollFrame parentKey="CurrencyScroll" inherits="GwHybridScrollFrameTemplate">
                <Size x="580" y="576"></Size>
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="-2" y="-10"></Anchor>
                </Anchors>
            </ScrollFrame>
            <ScrollFrame parentKey="RaidScroll" inherits="GwHybridScrollFrameTemplate" hidden="true">
                <Size x="580" y="576"></Size>
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="-2" y="-10"></Anchor>
                </Anchors>
            </ScrollFrame>
        </Frames>
    </Frame>

</Ui>