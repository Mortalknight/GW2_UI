<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
    <Button name="GwBuffIconBig" frameStrata="LOW" toplevel="true" virtual="true">
        <Size x="26" y="26"/>
        <Layers>
            <Layer level="BACKGROUND">
                <Texture parentKey="backdrop" file="Interface/AddOns/GW2_UI/textures/uistuff/spelliconempty">
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="0" y="0"/>
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0"/>
                    </Anchors>
                </Texture>
            </Layer>
            <Layer>
                <Texture parentKey="buffIcon" file="Interface/AddOns/GW2_UI/textures/uistuff/gwstatusbar">
                    <TexCoords left="0.1" right="0.9" top="0.1" bottom="0.9" />
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="1" y="-1"/>
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="-1" y="1"/>
                    </Anchors>
                </Texture>
                <FontString parentKey="buffDuration" text="" inherits="GameFontNormalSmall" justifyH="CENTER">
                    <Size x="32" y="12"/>
                    <Anchors>
                        <Anchor point="BOTTOM" x="0" y="-12"/>
                    </Anchors>
                </FontString>
                <FontString parentKey="buffStacks" text="" inherits="GameFontNormalSmall" justifyH="CENTER">
                    <Size x="32" y="32"/>
                    <Anchors>
                        <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="0" y="0"/>
                        <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0"/>
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
    </Button>
</Ui>
