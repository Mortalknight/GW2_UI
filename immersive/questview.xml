<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
    <Frame name="GwQuestviewFrameTemplate" hidden="true" frameStrata="DIALOG" enableMouse="true" virtual="true">
        <Size x="1024" y="512"></Size>
        <Anchors>
            <Anchor point="center" x="0" y="0" />
        </Anchors>
        <Layers>
            <Layer level="BACKGROUND" textureSubLevel="1">
                <Texture file="Interface\AddOns\GW2_UI\textures\questview\border">
                    <Size x="1024" y="512"></Size>
                    <Anchors>
                        <Anchor point="center" x="0" y="0" />
                    </Anchors>
                </Texture>
            </Layer>
            <Layer level="BACKGROUND" textureSubLevel="2">
                <Texture parentKey="mapBG" file="Interface/AddOns/GW2_UI/textures/questview/backgrounds/default">
                    <Size x="950" y="475"></Size>
                    <Anchors>
                        <Anchor point="center" x="0" y="0" />
                    </Anchors>
                </Texture>
            </Layer>
            <Layer level="BACKGROUND" textureSubLevel="3">
                <Texture file="Interface\AddOns\GW2_UI\textures\questview\overlay_top">
                    <Size x="950" y="237"></Size>
                    <Anchors>
                        <Anchor point="center" x="0" y="119" />
                    </Anchors>
                </Texture>
            </Layer>
            <Layer level="OVERLAY">
                <FontString parentKey="title" inherits="GameFontNormal" text="Test text" justifyV="MIDDLE">
                    <Size x="950" y="100"></Size>
                    <Anchors>
                        <Anchor point="TOP" relativePoint="TOP" x="0" y="0" />
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>

        <Frames>
            <Frame parentKey="container">
                <Size x="950" y="474"></Size>
                <Anchors>
                    <Anchor point="center" x="0" y="0" />
                </Anchors>
                <Frames>
                    <PlayerModel parentKey="playerModel" frameStrata="DIALOG">
                        <Size x="750" y="474"></Size>
                        <Anchors>
                            <Anchor point="BOTTOMLEFT" relativePoint="BOTTOMLEFT" x="0" y="1" />
                        </Anchors>
                    </PlayerModel>
                    <PlayerModel parentKey="giverModel">
                        <Size x="750" y="474"></Size>
                        <Anchors>
                            <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="1" />
                        </Anchors>
                    </PlayerModel>

                    <Frame parentKey="dialog" frameStrata="DIALOG" enableMouse="true">
                        <Size x="950" y="374"></Size>
                        <Anchors>
                            <Anchor point="center" x="0" y="-50" />
                        </Anchors>
                        <Layers>
                            <Layer level="OVERLAY">
                                <Texture file="Interface\AddOns\GW2_UI\textures\questview\overlay_bottom">
                                    <Size x="952" y="475"></Size>
                                    <Anchors>
                                        <Anchor point="bottom" x="0" y="-1" />
                                    </Anchors>
                                </Texture>
                            </Layer>
                            <Layer level="OVERLAY" textureSubLevel="2">
                                <FontString parentKey="text" inherits="GameFontNormal" text="Test text" justifyV="MIDDLE">
                                    <Size x="850" y="100"></Size>
                                    <Anchors>
                                        <Anchor point="BOTTOM" relativePoint="BOTTOM" x="0" y="0" />
                                    </Anchors>
                                </FontString>
                                <FontString parentKey="objectiveHeader" inherits="QuestTitleFont" justifyH="LEFT" hidden="true">
                                    <Anchors>
                                        <Anchor point="LEFT" relativePoint="CENTER" x="-150" y="160" />
                                    </Anchors>
                                </FontString>
                                <FontString parentKey="objectiveText" inherits="QuestFont" justifyH="LEFT" hidden="true">
                                    <Size x="300" y="0"/>
                                    <Anchors>
                                        <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT" relativeKey="$parent.objectiveHeader" x="0" y="-5" />
                                    </Anchors>
                                </FontString>
                                <FontString parentKey="required" inherits="QuestTitleFont" justifyH="LEFT" hidden="true">
                                    <Anchors>
                                        <Anchor point="LEFT" relativePoint="CENTER" x="-150" y="70" />
                                    </Anchors>
                                </FontString>
                            </Layer>
                        </Layers>
                        <Frames>
                            <Frame inherits="MoneyFrameTemplate" parentKey="reqMoney">
                                <Anchors>
                                    <Anchor point="TOPLEFT" relativeKey="$parent.required" relativePoint="BOTTOMLEFT" x="5" y="-5"/>
                                </Anchors>
                                <Scripts>
                                    <OnLoad>
                                        SmallMoneyFrame_OnLoad(self);
                                        MoneyFrame_SetType(self, "STATIC");
                                    </OnLoad>
                                </Scripts>
                            </Frame>
                            <Button name="GwReqQuestItem1" inherits="QuestItemTemplate" parentKey="reqItem1">
                                <Anchors>
                                    <Anchor point="TOPLEFT" relativeKey="$parent.reqMoney" relativePoint="BOTTOMLEFT" x="0" y="-5"/>
                                </Anchors>
                            </Button>
                            <Button name="GwReqQuestItem2" inherits="QuestItemTemplate" parentKey="reqItem2">
                                <Anchors>
                                    <Anchor point="LEFT" relativeKey="$parent.reqItem1" relativePoint="RIGHT" x="2" y="0"/>
                                </Anchors>
                            </Button>
                            <Button name="GwReqQuestItem3" inherits="QuestItemTemplate" parentKey="reqItem3">
                                <Anchors>
                                    <Anchor point="TOP" relativeKey="$parent.reqItem1" relativePoint="BOTTOM" x="0" y="-3"/>
                                </Anchors>
                            </Button>
                            <Button name="GwReqQuestItem4" inherits="QuestItemTemplate" parentKey="reqItem4">
                                <Anchors>
                                    <Anchor point="LEFT" relativeKey="$parent.reqItem3" relativePoint="RIGHT" x="2" y="0"/>
                                </Anchors>
                            </Button>
                            <Button name="GwReqQuestItem5" inherits="QuestItemTemplate" parentKey="reqItem5">
                                <Anchors>
                                    <Anchor point="TOP" relativeKey="$parent.reqItem3" relativePoint="BOTTOM" x="0" y="-3"/>
                                </Anchors>
                            </Button>
                            <Button name="GwReqQuestItem6" inherits="QuestItemTemplate" parentKey="reqItem6">
                                <Anchors>
                                    <Anchor point="LEFT" relativeKey="$parent.reqItem5" relativePoint="RIGHT" x="2" y="0"/>
                                </Anchors>
                            </Button>
                        </Frames>
                    </Frame>

                    <Button parentKey="acceptButton" inherits="GwStandardButton">
                        <Size x="120" y="24" />
                        <Anchors>
                            <Anchor point="TOPLEFT" relativePoint="BOTTOMLEFT"></Anchor>
                        </Anchors>

                    </Button>
                    <Button parentKey="declineButton" text="Decline" inherits="GwStandardButton">
                        <Size x="120" y="24" />
                        <Anchors>
                            <Anchor point="TOPRIGHT" relativePoint="BOTTOMRIGHT"></Anchor>
                        </Anchors>
                    </Button>

                </Frames>
            </Frame>


        </Frames>

    </Frame>


</Ui>