            </Layer>
        </Layers>

        <Frames>
            <Button parentKey="ItemTab">
                <Size x="40" y="40"></Size>
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="-40" y="-20"></Anchor>
                </Anchors>
                <Layers>
                    <Layer level="OVERLAY">
                        <Texture parentKey="Icon" file="Interface/AddOns/GW2_UI/textures/bag/bankicons">
                            <TexCoords left="0" right="0.5" top="0.5" bottom="1" />
                            <Size x="32" y="32"></Size>
                            <Anchors>
                                <Anchor point="CENTER" x="0" y="0"></Anchor>
                            </Anchors>
                        </Texture>
                    </Layer>
                </Layers>
            </Button>
            <Button parentKey="ReagentTab">
                <Size x="40" y="40"></Size>
                <Anchors>
                    <Anchor point="TOPLEFT" relativePoint="TOPLEFT" x="-40" y="-60"></Anchor>
                </Anchors>
                <Layers>
                    <Layer level="OVERLAY">
                        <Texture parentKey="Icon" file="Interface/AddOns/GW2_UI/textures/bag/bankicons">
                            <TexCoords left="0" right="0.5" top="0" bottom="0.5" />
                            <Size x="32" y="32"></Size>
                            <Anchors>
                                <Anchor point="CENTER" x="0" y="0"></Anchor>
                            </Anchors>
                        </Texture>
                    </Layer>
