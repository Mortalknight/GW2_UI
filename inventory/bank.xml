                            <Anchors>
                                <Anchor point="BOTTOMRIGHT" x="0" y="0"></Anchor>
                            </Anchors>
                        </Texture>
                    </Layer>
                </Layers>
            </Frame>
            <Button parentKey="buttonSettings" text="">
