                    <Anchor point="BOTTOMRIGHT" relativePoint="BOTTOMRIGHT" x="-4" y="3"></Anchor>
                </Anchors>
                <Frames>

                    <Frame parentKey="Header" inherits="BankPanelHeaderFrameTemplate" hidden="true">
						<Anchors>
							<Anchor point="TOP" y="50"/>
						</Anchors>
					</Frame>

                    <Frame parentKey="MoneyFrame" inherits="BankPanelMoneyFrameTemplate" hidden="true">
						<Anchors>
							<Anchor point="BOTTOMRIGHT" x="-3" y="3"/>
						</Anchors>
					</Frame>

                    <Frame parentKey="ItemDepositFrame" inherits="BankPanelItemDepositFrameTemplate">
						<Anchors>
							<Anchor point="BOTTOM" relativeKey="$parent.NineSlice" relativePoint="BOTTOM" y="10"/>
						</Anchors>
					</Frame>

                    <ItemButton parentKey="PurchaseTab" inherits="BankPanelPurchaseTabTemplate" hidden ="true"/>

                    <Frame parentKey="PurchasePrompt" inherits="BankPanelPurchasePromptTemplate" hidden="true" parentArray="Prompts">
						<Anchors>
							<Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT" x="0" y="0"/>
							<Anchor point="BOTTOMRIGHT" relativeKey="$parent" relativePoint="BOTTOMRIGHT" x="0" y="0"/>
						</Anchors>
					</Frame>

					<Frame parentKey="LockPrompt" inherits="BankPanelLockPromptTemplate" hidden="true" parentArray="Prompts">
						<Anchors>
							<Anchor point="TOPLEFT" relativeKey="$parent" relativePoint="TOPLEFT" x="0" y="0"/>
							<Anchor point="BOTTOMRIGHT" relativeKey="$parent" relativePoint="BOTTOMRIGHT" x="0" y="0"/>
						</Anchors>
					</Frame>

                </Frames>
                <Scripts>
					<OnLoad method="OnLoad"/>
					<OnShow method="OnShow"/>
					<OnHide method="OnHide"/>
					<OnEvent method="OnEvent"/>
					<OnUpdate method="OnUpdate"/>
				</Scripts>
