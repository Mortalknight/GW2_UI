                        <Frames>
                            <Button parentKey="compactBank">
                                <Size x="200" y="30"></Size>
                                <Anchors>
                                    <Anchor point="TOPLEFT" relativePoint="TOPLEFT" y="0"></Anchor>
                                </Anchors>
                                <Layers>
                                    <Layer level="OVERLAY" textureSubLevel="2">
                                        <FontString parentKey="title" text="Compact view" inherits="ChatBubbleFont" justifyH="LEFT">
                                            <Size x="170" y="30"/>
                                            <Color r="1" b="1" g="1"/>
                                            <Anchors>
                                                <Anchor point="LEFT" x="25" y="0"/>
                                            </Anchors>
                                        </FontString>
                                    </Layer>
                                </Layers>
                                <Frames>
                                    <CheckButton parentKey="checkbutton" checked="false">
                                        <HitRectInsets left="0" right="0" top="0" bottom="0"/>
                                        <Size x="16" y="16"/>
                                        <Anchors>
                                            <Anchor point="LEFT" relativePoint="LEFT" x="5"/>
                                        </Anchors>
                                        <NormalTexture file="Interface/AddOns/GW2_UI/textures/checkbox"/>
                                        <CheckedTexture file="Interface/AddOns/GW2_UI/textures/checkboxchecked"/>
                                    </CheckButton>
                                </Frames>
                            </Button>
                            <Button parentKey="bagOrder">
                                <Size x="200" y="30"></Size>
                                <Anchors>
                                    <Anchor point="TOPLEFT" relativePoint="TOPLEFT" y="-25"></Anchor>
                                </Anchors>
                                <Layers>
                                    <Layer level="OVERLAY" textureSubLevel="2">
                                        <FontString parentKey="title" text="Reverse Bag Order" inherits="ChatBubbleFont" justifyH="LEFT">
                                            <Size x="170" y="30"/>
                                            <Color r="1" b="1" g="1"/>
                                            <Anchors>
                                                <Anchor point="LEFT" x="25" y="0"/>
                                            </Anchors>
                                        </FontString>
                                    </Layer>
                                </Layers>
                                <Frames>
                                    <CheckButton parentKey="checkbutton" checked="false">
                                        <HitRectInsets left="0" right="0" top="0" bottom="0"/>
                                        <Size x="16" y="16"/>
                                        <Anchors>
                                            <Anchor point="LEFT" relativePoint="LEFT" x="5"/>
                                        </Anchors>
                                        <NormalTexture file="Interface/AddOns/GW2_UI/textures/checkbox"/>
                                        <CheckedTexture file="Interface/AddOns/GW2_UI/textures/checkboxchecked"/>
                                    </CheckButton>
                                </Frames>
                            </Button>
                        </Frames>
                    </Frame>
                </Frames>
            </Button>
            <Button parentKey="buttonSort" text="">
