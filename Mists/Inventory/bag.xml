                </Anchors>
                <NormalTexture file="Interface/AddOns/GW2_UI/textures/icons/currency-icon.png"></NormalTexture>
            </Button>
            <Button parentKey="moneyFrame" registerForClicks="RightButtonUp">
                <Size x="180" y="40"></Size>
                <Anchors>
                    <Anchor point="BOTTOMRIGHT" x="0" y="-40"></Anchor>
                </Anchors>
                <Scripts>
                    <OnLeave>
                        GameTooltip:Hide()
                    </OnLeave>
                </Scripts>
            </Button>
            <Button parentKey="currency1Frame">
                <Size x="50" y="40"></Size>
                <Anchors>
